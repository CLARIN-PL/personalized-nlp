[flake8]
max_line_length = 90
docstring-convention = "google"
inline_quotes = "
ignore =
    DAR101
    # Due to issue https://github.com/terrencepreilly/darglint/issues/54
    DAR202
    # This violation will be updated to follow PEP 8 guidelines
    W503
per-file-ignores =
    # Imported but unused
    __init__.py:F401
